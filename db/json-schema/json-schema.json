{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "name": {
          "type": [
            "string",
            "null"
          ]
        },
        "email": {
          "type": "string"
        },
        "hashedPassword": {
          "type": [
            "string",
            "null"
          ]
        },
        "role": {
          "type": "string",
          "default": "USER"
        },
        "tokens": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Token"
          }
        },
        "sessions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        }
      }
    },
    "Session": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "expiresAt": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "handle": {
          "type": "string"
        },
        "hashedSessionToken": {
          "type": [
            "string",
            "null"
          ]
        },
        "antiCSRFToken": {
          "type": [
            "string",
            "null"
          ]
        },
        "publicData": {
          "type": [
            "string",
            "null"
          ]
        },
        "privateData": {
          "type": [
            "string",
            "null"
          ]
        },
        "user": {
          "anyOf": [
            {
              "$ref": "#/definitions/User"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "Token": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "hashedToken": {
          "type": "string"
        },
        "type": {
          "type": "string",
          "enum": [
            "RESET_PASSWORD"
          ]
        },
        "expiresAt": {
          "type": "string",
          "format": "date-time"
        },
        "sentTo": {
          "type": "string"
        },
        "user": {
          "$ref": "#/definitions/User"
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "user": {
      "$ref": "#/definitions/User"
    },
    "session": {
      "$ref": "#/definitions/Session"
    },
    "token": {
      "$ref": "#/definitions/Token"
    }
  }
}